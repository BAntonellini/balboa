        {
            "authenticator": "SNOWFLAKE",
            "ocspOptions": {
                "ocspFailOpen": true
            },
            "snowflakeOptions": {
                "clientSessionKeepAlive": true,
                "clientSessionKeepAliveHeartbeatFrequency": 3600,
                "schema": "PUBLIC",
                "role": "ANALYST"
            },
            "previewLimit": 50,
            "driver": "Snowflake",
            "name": "dev_balboa",
            "account": "toa80779",
            "username": "{{ snowflake_username }}",
            "password": "{{ snowflake_password }}",
            "database": "DEV_BALBOA",
            "warehouse": "TRANSFORMING_WAREHOUSE"
        },
        {
            "authenticator": "SNOWFLAKE",
            "ocspOptions": {
                "ocspFailOpen": true
            },
            "snowflakeOptions": {
                "clientSessionKeepAlive": true,
                "clientSessionKeepAliveHeartbeatFrequency": 3600,
                "schema": "PUBLIC",
                "role": "ANALYST"
            },
            "previewLimit": 50,
            "driver": "Snowflake",
            "name": "dev_raw",
            "account": "toa80779",
            "username": "{{ snowflake_username }}",
            "password": "{{ snowflake_password }}",
            "database": "DEV_RAW",
            "warehouse": "TRANSFORMING_WAREHOUSE"
        }
    ],
    "sqltools.tableTreeItemsExpanded": true,
    "sqltools.showStatusbar": false,
    "sqltools.format": {
        "reservedWordCase": "lower",
        "language": "sql"
    },
    "terminal.integrated.cwd": "/config/workspace/transform",
    "actionButtons": {
        "defaultColor": "#ff0034", // Can also use string color names.
			"loadNpmCommands":false, // Disables automatic generation of actions for npm commands.
			"reloadButton": null,
			"commands": [
                {
					"name": "‚ñ∂Ô∏è Build current",
                    "cwd": "/config/workspace/transform",
                    "color": "white",
					"singleInstance": true,
					"command": "dbt build --select ${fileBasenameNoExtension} --defer --state logs"
				},
				{
                    "name": "‚è≠ Build changes",
                    "cwd": "/config/workspace/transform",
					"color": "white",
					"singleInstance": true,
					"command": "dbt build --select state:modified+ --defer --state logs"
				},
                {
					"name": "‚úîÔ∏è Run checks",
                    "cwd": "/config/workspace/transform",
                    "color": "white",
					"singleInstance": true,
					"command": "dbt-coves check" // This is executed in the terminal.
				},
                {
					"name": "‚úîÔ∏è lint current",
                    "cwd": "/config/workspace/transform",
                    "color": "white",
					"singleInstance": true,
					"command": "sqlfluff lint ${file}" // This is executed in the terminal.
				},
                {
                    "name": "üìù Create YML for current",
                    "cwd": "${fileDirname}",
                    "color": "white",
                    "singleInstance": true,
                    "command": "dbt run-operation gen_yaml --args '{'model_name': '${fileBasenameNoExtension}'}' | tail -n +2 | grep . > ${fileBasenameNoExtension}.yml" // This is executed in the terminal.
                },
                {
					"name": "üó≥Ô∏è Prod metadata",
                    "cwd": "/config/workspace/transform",
                    "color": "white",
					"singleInstance": true,
					"command": "/config/workspace/automate/dbt/get_artifacts.sh"
				}
			]
    }
}